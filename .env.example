# AirMessage Web Environment Configuration
# Copy this file to .env and update with your values

# AirMessage Connect Server
# WebSocket URL to your Connect server (e.g., wss://connect.yourdomain.com)
REACT_APP_CONNECT_HOSTNAME=

# Google API Configuration
# Get these from Google Cloud Console > APIs & Services > Credentials
REACT_APP_GOOGLE_API_KEY=
REACT_APP_GOOGLE_CLIENT_ID=
REACT_APP_GOOGLE_CLIENT_SECRET=

# Firebase Configuration
# Get these from Firebase Console > Project Settings > General
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=

# Sentry DSN (optional)
# Get this from Sentry.io > Settings > Projects > Client Keys (DSN)
# REACT_APP_SENTRY_DSN=

# Local Encryption Key
# Generate a random 16-byte base64 string for local data encryption
REACT_APP_JWK_LOCAL_ENCRYPTION_KEY=
