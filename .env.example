# AirMessage Web Environment Configuration
#
# SETUP: Copy this file to .env and fill out the placeholders with your actual values
#

# Your AirMessage Connect server WebSocket URL (e.g., wss://connect.yourdomain.com)
REACT_APP_CONNECT_HOSTNAME=

# Get from Google Cloud Console - API Key for Google services
REACT_APP_GOOGLE_API_KEY=
# Get from Google Cloud Console - OAuth 2.0 Client ID
REACT_APP_GOOGLE_CLIENT_ID=
# Get from Google Cloud Console - OAuth 2.0 Client Secret
REACT_APP_GOOGLE_CLIENT_SECRET=

# Get from Firebase Console - Project Settings - General
REACT_APP_FIREBASE_API_KEY=
# Firebase Auth Domain (projectid.firebaseapp.com)
REACT_APP_FIREBASE_AUTH_DOMAIN=
# Firebase Project ID
REACT_APP_FIREBASE_PROJECT_ID=
# Firebase Storage Bucket (projectid.appspot.com)
REACT_APP_FIREBASE_STORAGE_BUCKET=
# Firebase Messaging Sender ID
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
# Firebase App ID
REACT_APP_FIREBASE_APP_ID=

# Optional: Sentry DSN for error tracking
# REACT_APP_SENTRY_DSN=

# Generate a random base64 string for local encryption
REACT_APP_JWK_LOCAL_ENCRYPTION_KEY=
